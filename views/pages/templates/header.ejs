    <script>
   
      function getCookie(name) {
        console.log(document.cookie.indexOf(name));
        var value = "; " + document.cookie;
        var parts = value.split("; " + name + "=");
        if (parts.length == 2) return parts.pop().split(";").shift();
      }
      
      function setLoginCookies(callback) {
        var username = $("#username").val();
        var password = $("#password").val();
        if(password == "" || username ==""){
          alert("enter a login");
          return;
        }
      
        var parameters = {username: username, password: password};
        $.get('/getLogin', parameters, function(data){
         // console.log("Sending Login credentials");
        });
        
      }
      
      function logout() {
        console.log("Log out");
        var cookies = document.cookie.split(";");
        for (var i = 0; i < cookies.length; i++) {
            var cookie = cookies[i];
            var eqPos = cookie.indexOf("=");
            var name = eqPos > -1 ? cookie.substr(0, eqPos) : cookie;
            document.cookie = name + "=;expires=Thu, 01 Jan 1970 00:00:00 GMT";
        }
        
        $("#title").html("<%= title %>");
        $("#loginLink").removeClass("hidden");
        $("#signupLink").removeClass("hidden");
        $("#logoutLink").addClass("hidden");
      }
      
      function setHeaders() {
        console.log(getCookie("username"));
        if(getCookie("username")) {
          $("#title").html("Hello " + getCookie("username"));
          $("#loginLink").addClass("hidden");
          $("#signupLink").addClass("hidden");
          $("#logoutLink").removeClass("hidden");
        }
      }
      
      setHeaders();
    </script>
